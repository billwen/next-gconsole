{
  "name": "next-gconsole",
  "version": "0.2.2",
  "private": true,
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "dotenvx run -e .env.local -e .env -- prisma migrate dev --name dev",
    "db:push": "dotenvx run -e .env.local -e .env -- prisma db push",
    "db:reset": "dotenvx run -e .env.local -e .env -- prisma migrate reset --force",
    "db:seed": "dotenvx run -e .env.local -e .env  -- prisma db seed",
    "prisma:generate": "dotenvx run -e .env.local -e .env -- prisma generate",
    "studio": "dotenvx run -e .env.local -e .env -- prisma studio",
    "db:format": "dotenvx run -e .env.local -e .env -- prisma format",
    "prisma:validate": "dotenvx run -e .env.local -e .env -- prisma validate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.0",
    "@dotenvx/dotenvx": "^0.24.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.336.0",
    "nanoid": "^5.0.6",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.15",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.0.1",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
